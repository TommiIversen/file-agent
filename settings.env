# File Transfer Agent - Konfiguration
# =====================================

# Filstier
SOURCE_DIRECTORY=c:\temp_input
DESTINATION_DIRECTORY=c:\temp_output

# Timing konfiguration  
FILE_STABLE_TIME_SECONDS=10
POLLING_INTERVAL_SECONDS=10

# Filkopiering
USE_TEMPORARY_FILE=true
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY_SECONDS=10
GLOBAL_RETRY_DELAY_SECONDS=60

# Logging konfiguration
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/file_agent.log
LOG_RETENTION_DAYS=30
LOG_MAX_SIZE_MB=100
CONSOLE_LOG_FORMAT=colored
FILE_LOG_FORMAT=json